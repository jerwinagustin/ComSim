{
  "expo": {
    "name": "ComSim",
    "slug": "ComSim",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/COMSIM.png",  // Make sure the icon is square and at least 1024x1024px
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/COMSIM.png",  // Ensure this image is centered and square
        "backgroundColor": "#ffffff"  // Background color for adaptive icon
      },
      "package": "com.anonymous.ComSim"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/COMSIM.png",  // Correct splash screen image path
          "imageWidth": 200,
          "resizeMode": "contain",  // You can adjust this if you need the image to fit better
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f9f45f3a-d80b-4bab-96ec-f8caf5c7a103"
      }
    },
    "owner": "jerwin"
  }
}
